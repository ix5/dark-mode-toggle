{
  "manifest_version": 2,
  "name": "dark-mode-toggle",
  "description": "Toggle dark mode for the current page",
  "version": "0.1",
  "icons": {
    "64": "icons/icon.png"
  },
  "browser_action": {
    "default_icon": {
      "64": "icons/icon.png"
    },
    "default_title": "Dark Mode Toggle"
  },
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "background": {
    "scripts": ["background.js"]
  }
}
